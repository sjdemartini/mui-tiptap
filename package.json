{
  "name": "mui-tiptap",
  "version": "1.0.0-alpha.10",
  "description": "A Material-UI (MUI) styled WYSIWYG rich text editor, using Tiptap",
  "keywords": [
    "mui",
    "tiptap",
    "material-ui",
    "prosemirror",
    "rich text editor",
    "wysiwyg",
    "quill",
    "draftjs",
    "slate"
  ],
  "homepage": "https://github.com/sjdemartini/mui-tiptap",
  "bugs": "https://github.com/sjdemartini/mui-tiptap/issues",
  "author": "Steven DeMartini <sjdemartini@users.noreply.github.com>",
  "repository": "github:sjdemartini/mui-tiptap",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "license": "MIT",
  "engines": {
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.6.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsc --project tsconfig.build.json && tsc --project tsconfig.build-esm.json",
    "clean": "rimraf dist coverage",
    "example": "pnpm build && rimraf ./example/node_modules && pnpm --dir ./example install && pnpm --dir ./example run dev",
    "dev": "vite",
    "type:check": "tsc --noEmit",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx src",
    "preinstall": "npx only-allow pnpm",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md}\"",
    "prepare": "husky install"
  },
